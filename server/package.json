    "test:coverage": "jest --coverage",
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "echo \"No build step required for Node.js\"",
    "lint": "eslint src/",
    "type-check": "node -e \"console.log('No TS in server')\"",
    "migrate": "node scripts/update-database.js",
    "seed": "node scripts/seed.js",
    "perf:test": "node scripts/performance-test.js",
    "perf:test:quick": "TOTAL_REQUESTS=50 CONCURRENT_REQUESTS=5 node scripts/performance-test.js",
    "perf:test:heavy": "TOTAL_REQUESTS=500 CONCURRENT_REQUESTS=50 node scripts/performance-test.js",
    "start:optimized": "node --expose-gc --max-old-space-size=1024 src/index.js"
  },
