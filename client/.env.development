# BioVerse Client Development Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_APP_NAME=BioVerse
VITE_APP_VERSION=1.0.0

# Google OAuth (Optional - for Google Sign-In)
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Firebase Configuration (Optional - for real-time features)
VITE_FIREBASE_API_KEY=demo-firebase-key
VITE_FIREBASE_AUTH_DOMAIN=bioverse-demo.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=bioverse-demo
VITE_FIREBASE_STORAGE_BUCKET=bioverse-demo.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:demo-app-id

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_3D_VISUALIZATIONS=true
VITE_ENABLE_REAL_TIME=true
VITE_DEMO_MODE=true

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
